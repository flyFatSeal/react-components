{
  "name": "haiwell",
  "version": "0.1.11",
  "description": "React components library",
  "authors": {
    "name": "fatseal",
    "email": "491927227@qq.com"
  },
  "private": false,
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "homepage": "http://haiwell.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/flyFatSeal/react-components"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "conventional-changelog-cli": "^2.1.0",
    "handsontable": "^6.2.2",
    "omit.js": "^2.0.2",
    "react-transition-group": "^4.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "clean": "rimraf ./lib",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "build": "npm run clean && npm run build-ts && npm run copyLess",
    "copyLess": "copyfiles -u 1 \"src/components/**/style/*.less\" lib && copyfiles -u 1 \"src/components/style/**/*.less\" lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "new": "plop --plopfile ./scripts/plopfile.ts",
    "release": "ts-node ./scripts/release.ts",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/semver": "^7.3.3",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.48",
    "@types/react-dom": "16.9.4",
    "@types/react-transition-group": "^4.4.0",
    "@types/storybook__addon-info": "^5.2.1",
    "commitlint-config-cz": "^0.13.2",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "cz-customizable": "^6.3.0",
    "husky": "^4.2.5",
    "inquirer": "^7.0.4",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "plop": "^2.7.4",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-scripts": "3.2.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.3",
    "webpack": "^4.44.1"
  }
}
